<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rick.jinmall.mapper.UserInfoMapper">
    <insert id="insertUserInfo" parameterType="com.rick.jinmall.bean.UserInfo">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            select LAST_INSERT_ID()
        </selectKey>
        insert into tb_user(name,sex,age,password,birthday,account,user_phone,address,user_wechat,user_qq,user_email)
        value(#{name},#{sex},#{age},#{password},#{birthday},#{account},#{user_phone},#{address},#{user_wechat},#{user_qq},#{user_email})
    </insert>


    <select id="getUserInfo" resultType="com.rick.jinmall.bean.UserInfo">
        select name, age, password from tb_user;
    </select>
</mapper>